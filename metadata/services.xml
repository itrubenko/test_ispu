<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="int_ispu.creds">
        <url>https://api.bring.com/pickuppoint/api/pickuppoint/DK/postalCode</url>
        <user-id/>
        <password/>
    </service-credential>

    <service-profile service-profile-id="int_common_service.profile">
        <timeout-millis>60000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>10</rate-limit-calls>
        <rate-limit-millis>3000</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>5</cb-calls>
        <cb-millis>2000</cb-millis>
    </service-profile>

    <service service-id="int_ispu.https">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>int_ispu</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>int_common_service.profile</profile-id>
        <credential-id>int_ispu.creds</credential-id>
    </service>
</services>
