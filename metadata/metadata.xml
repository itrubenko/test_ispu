<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
             <attribute-definition attribute-id="mapAPI">
                <display-name xml:lang="x-default">google map api key</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </custom-attribute-definitions>
		<group-definitions>
            <attribute-group group-id="Storefront Configs">
                <display-name xml:lang="x-default">Storefront Configurations</display-name>
                <attribute attribute-id="mapAPI"/>
            </attribute-group>
		 </group-definitions>
    </type-extension>
	
   <type-extension type-id="OrderAddress">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="fromStoreId">
                <display-name xml:lang="x-default">Store ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="isStore">
                <display-name xml:lang="x-default">Is store</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Standard">
                <display-name xml:lang="x-default">Standard Address</display-name>
                <attribute attribute-id="isStore"/>
                <attribute attribute-id="fromStoreId"/>
            </attribute-group>
        </group-definitions>
    </type-extension>

    <type-extension type-id="ProductLineItem">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="fromStoreId">
                <display-name xml:lang="x-default">From Store ID</display-name>
                <description xml:lang="x-default">Links the store to the product line item for grouping shipments in the checkout process.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="storeInfo">
                <display-name xml:lang="x-default">Store Info</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </custom-attribute-definitions>
    </type-extension>

    <type-extension type-id="Shipment">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="fromStoreId">
                <display-name xml:lang="x-default">Pickup Store ID</display-name>
                <description xml:lang="x-default">Used to map the shipment to a  brick and mortar store.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
            </attribute-definition>
            <attribute-definition attribute-id="storeInfo">
                <display-name xml:lang="x-default">Store Info</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="storePickupMessage">
                <display-name xml:lang="x-default">Store Pickup Message</display-name>
                <description xml:lang="x-default">This is text used by the end user when sending a message to the brick and mortar store about the shipment.  This is reflected in the order export.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="storepickup">
                <display-name xml:lang="x-default">Store Pickup Information</display-name>
                <attribute attribute-id="fromStoreId"/>
                <attribute attribute-id="storePickupMessage"/>
                <attribute attribute-id="storeInfo"/>
            </attribute-group>
        </group-definitions>
    </type-extension>

    <type-extension type-id="ShippingMethod">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="pickupEnabled">
                <display-name xml:lang="x-default">Pick Up</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="storePickupEnabled">
                <display-name xml:lang="x-default">Store Pickup Enabled</display-name>
                <type>boolean</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="storePickUp">
                <display-name xml:lang="x-default">Store Pick Up</display-name>
                <attribute attribute-id="storePickupEnabled"/>
                <attribute attribute-id="pickupEnabled"/>
            </attribute-group>
        </group-definitions>
    </type-extension>

	<type-extension type-id="Product">
		<custom-attribute-definitions>
            <attribute-definition attribute-id="availableForInStorePickup">
                <display-name xml:lang="x-default">Available for In Store Pickup</display-name>
                <description xml:lang="x-default">Signals if there are inventory lists for brick-and-mortar stores associated with this product.</description>
                <type>boolean</type>
                <site-specific-flag>false</site-specific-flag>
                <mandatory-flag>false</mandatory-flag>
                <visible-flag>false</visible-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <order-required-flag>false</order-required-flag>
                <externally-defined-flag>false</externally-defined-flag>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="inStorePickup">
                <display-name xml:lang="x-default">In Store Pickup</display-name>
                <attribute attribute-id="availableForInStorePickup"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>
